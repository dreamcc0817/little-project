    <build>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
                <configuration>
                    <mainClass>com.dreamcc.little.project.LittleProjectApplication</mainClass>
                    <excludes>
                        <exclude>
